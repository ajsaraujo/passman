<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root spacing="20.0" type="VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="New Passman file created">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" prefWidth="351.0" text="A Passman file was successfully created at &lt;path&gt;." wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="proceedButton" mnemonicParsing="false" onAction="#proceedButtonClicked" prefHeight="36.0" prefWidth="96.0" text="Proceed" />
   </children>
   <padding>
      <Insets bottom="35.0" left="50.0" right="50.0" top="35.0" />
   </padding>
</fx:root>
