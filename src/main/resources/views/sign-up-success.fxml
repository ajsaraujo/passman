<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.SignUpSuccess">
   <children>
      <Label text="Signed up successfully">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label prefHeight="51.0" prefWidth="350.0" text="Your account was successfully created. You can now proceed to the login screen." wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <AnchorPane>
         <children>
            <Button mnemonicParsing="false" prefHeight="34.0" prefWidth="307.0" text="Continue" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="35.0" left="50.0" right="50.0" top="35.0" />
   </padding>
</VBox>
