<?xml version="1.0" encoding="UTF-8"?>

<?import com.passman.ui.components.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Import Passman file">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </Label>
      <OpenFileField />
      <FormField labelText="File password:" required="true">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></FormField>
      <ConfirmOrCancel />
   </children>
   <padding>
      <Insets bottom="40.0" left="50.0" right="60.0" top="35.0" />
   </padding>
</VBox>
